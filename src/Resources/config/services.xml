<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <!-- ENTITY DEFINITIONS -->
        <service id="Topdata\TopdataFileAttachmentsSW6\Entity\TopdataFaProductDocumentDefinition">
            <tag name="shopware.entity.definition" entity="topdata_fa_product_document" />
        </service>

        <!-- CONTROLLER -->
        <service id="Topdata\TopdataFileAttachmentsSW6\Storefront\Controller\FileDownloadController" public="true" autowire="true">
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>

    </services>
</container>
